{
  "mcpServers": {
    "selfie-mcp-server": {
      "command": "npm",
      "args": ["run", "mcp-server"],
      "env": {}
    },
    "playwright-mcp-server": {
      "command": "npx",
      "args": ["playwright-mcp-server"],
      "env": {}
    },
    "browsermcp": {
      "command": "npx",
      "args": ["@browsermcp/mcp@latest"]
    }
  }
}
